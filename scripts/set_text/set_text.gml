///set_text(font,alpha,color,halign,valign)
///@param font
///@param alpha
///@param color
///@param halign
///@param valign

function set_text(argument0, argument1, argument2, argument3, argument4)
{
	draw_set_font(argument0)
	
	draw_set_alpha(argument1)
	draw_set_color(argument2)
	draw_set_halign(argument3)
	draw_set_valign(argument4)
}